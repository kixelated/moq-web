{
	"name": "@kixelated/moq",
	"version": "0.2.0",
	"type": "module",
	"module": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"import": "./lib/index.js"
		},
		"./element/video": {
			"import": "./lib/element/video.js",
			"types": "./lib/element/video.d.ts"
		}
	},
	"files": ["lib", "pkg", "README.md", "LICENSE*"],
	"scripts": {
		"pack": "wasm-pack build --release && tsc",
		"dev": "npm-run-all --parallel dev:*",
		"dev:ts": "tsc -w",
		"dev:wasm": "cargo watch -s 'wasm-pack build --dev'",
		"lint": "biome check --write && cargo clippy --fix --allow-dirty --allow-staged",
		"fmt": "biome format --write && cargo fmt"
	},
	"dependencies": {
		"comlink": "^4.4.2",
		"npm-run-all": "^4.1.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"typescript": "^5.6.3"
	},
	"engines": {
		"bun": "1",
		"node": "use bun",
		"npm": "use bun",
		"yarn": "use bun",
		"pnpm": "use bun"
	},
	"engineStrict": true
}

{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": [
    "moq-element/**/*.ts",
    "moq-demo/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
